{
  "name": "@e18e/cli",
  "version": "0.0.1",
  "type": "module",
  "description": "The official e18e CLI",
  "bin": {
    "e18e-cli": "cli.js"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "browser": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib",
    "!lib/test"
  ],
  "scripts": {
    "cli": "node ./lib/cli.js",
    "clean": "premove lib coverage",
    "build:js": "npm run clean && tsc",
    "build": "npm run lint && npm run build:js",
    "lint": "eslint src",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "vitest run --coverage",
    "prepublishOnly": "npm run test",
    "generate-fixable-replacements": "node scripts/generate-fixable-replacements.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e18e/cli.git"
  },
  "keywords": [
    "e18e",
    "dependencies",
    "dependency",
    "publint"
  ],
  "author": "e18e Authors (https://e18e.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e18e/cli/issues"
  },
  "homepage": "https://github.com/e18e/cli#readme",
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "fast-wrap-ansi": "^0.2.0",
    "@publint/pack": "^0.1.4",
    "fdir": "^6.5.0",
    "gunshi": "^0.29.2",
    "lockparse": "^0.5.0",
    "module-replacements": "^2.11.0",
    "module-replacements-codemods": "^1.2.0",
    "obug": "^2.1.1",
    "package-manager-detector": "^1.6.0",
    "publint": "^0.3.17",
    "semver": "^7.7.4",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.3",
    "@types/picomatch": "^4.0.2",
    "@types/semver": "^7.7.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.2",
    "pkg-pr-new": "^0.0.65",
    "premove": "^4.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.3"
  }
}
